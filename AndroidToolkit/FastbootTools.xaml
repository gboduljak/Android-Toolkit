                            <TextBlock TextWrapping="Wrap"  x:Name="OutputBlock"/>
                        </ScrollViewer>
                        <TextBox x:Name="ExecuteOneCommandText"  Controls:TextboxHelper.ClearTextButton="True" HorizontalAlignment="Left" TextWrapping="Wrap"  VerticalAlignment="Top" Controls:TextboxHelper.Watermark="Enter Command Text here"/>
