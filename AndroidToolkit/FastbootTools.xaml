<Controls:MetroWindow x:Class="AndroidToolkit.FastbootTools"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        Title="Android Toolkit - Fastboot Tools" Height="560" Width="1000" ResizeMode="CanResizeWithGrip" WindowStartupLocation="CenterScreen"
        ShowIconOnTitleBar="true">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Fonts.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Controls.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/Blue.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/BaseDark.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Controls.AnimatedTabControl.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/FlatButton.xaml" />
                <ResourceDictionary Source="Resources/TextStyles.xaml" />
                <ResourceDictionary Source="Resources/Icons.xaml" />
                <ResourceDictionary Source="Resources/CustomButtonStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Controls:MetroWindow.WindowCommands>
        <Controls:WindowCommands>
            <Button x:Name="KillFastboot" Foreground="Gray" Content="KILL Fastboot" Click="KillFastboot_Click" />
            <Button x:Name="Settings" Click="Settings_Click" Foreground="Gray" >
                <StackPanel Orientation="Horizontal">
                    <Rectangle Width="20" Height="20" Fill="Gray"  >
                        <Rectangle.OpacityMask>
                            <VisualBrush Visual="{StaticResource appbar_settings}" Stretch="Fill"/>
                        </Rectangle.OpacityMask>
                    </Rectangle>
                </StackPanel>
            </Button>
            <Button>
                <StackPanel Orientation="Horizontal">
                    <Rectangle Width="20" Height="20" Fill="Gray">
                        <Rectangle.OpacityMask>
                            <VisualBrush Stretch="Fill" Visual="{StaticResource ResourceKey=appbar_os_android}" />
                        </Rectangle.OpacityMask>
                    </Rectangle>
                </StackPanel>
            </Button>
        </Controls:WindowCommands>
    </Controls:MetroWindow.WindowCommands>
    <Controls:MetroWindow.Flyouts>
        <Controls:FlyoutsControl>
            <Controls:Flyout x:Name="FastbootFlyout" IsOpen="False" Background="#9f000000">
                <Grid>
                    <StackPanel>
                        <Rectangle Fill="#FF41B1E1" Width="80" Height="80">
                            <Rectangle.OpacityMask>
                                <VisualBrush Visual="{StaticResource appbar_alert}" Stretch="Fill" />
                            </Rectangle.OpacityMask>
                        </Rectangle>
                        <ScrollViewer  Width="300" Height="300" CanContentScroll="True" VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Visible" Margin="20">
                            <TextBlock TextWrapping="Wrap" x:Name="FlyoutText"></TextBlock>
                        </ScrollViewer>
                    </StackPanel>

                </Grid>
            </Controls:Flyout>
            <Controls:Flyout x:Name="UISettingsFlyout" Header="UI Settings" Position="Bottom">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="20*"/>
                        <ColumnDefinition Width="50*"/>
                        <ColumnDefinition Width="30*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Margin="20">
                        <TextBlock Margin="10" Text="Make it your. ;)" TextWrapping="Wrap" Style="{StaticResource TileTitle}"/>
                    </Grid>
                    <Grid Grid.Column="1" Margin="20">
                        <DockPanel>
                            <TextBlock VerticalAlignment="Center" Margin="0,0,10,0" Text="SELECT ACCENT" TextWrapping="Wrap" Style="{StaticResource TileTitle}"/>
                            <ComboBox VerticalAlignment="Center" x:Name="Accents" SelectionChanged="Accents_SelectionChanged"/>
                        </DockPanel>

                    </Grid>

                    <Grid Grid.Column="2" Margin="10">
                        <Button Width="60" Height="60"  Style="{StaticResource MetroCircleButtonStyle}" x:Name="ChangeAccent" Click="ChangeAccent_Click">
                            <Controls:MetroImage Width="40" Height="40" Source="{StaticResource appbar_check}"/>
                        </Button>
                    </Grid>

                </Grid>

            </Controls:Flyout>
        </Controls:FlyoutsControl>

    </Controls:MetroWindow.Flyouts>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.30*"/>
            <RowDefinition Height="0.90*"/>
            <RowDefinition Height="0.02*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.90*"/>
            <ColumnDefinition Width="0.40*"/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="Title" Grid.Column="0">
            <StackPanel Margin="10">
                <DockPanel>
                    <StackPanel>

                        <TextBlock x:Name="TitleBig1" Style="{StaticResource ResourceKey=BigTitle}" Margin="20"><Run Text="Android Toolkit"/></TextBlock>
                    </StackPanel>

                    <Grid Margin="10,0,10,0">
                        <Rectangle Margin="10" Width="80" Height="80" Fill="{DynamicResource AccentColorBrush}">
                            <Rectangle.OpacityMask>
                                <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_settings}" />
                            </Rectangle.OpacityMask>
                        </Rectangle>

                    </Grid>
                    <TextBlock x:Name="TitleBig_Copy" Style="{StaticResource ResourceKey=BigTitle2}" Text="FASTBOOT" Height="65"/>

                </DockPanel>

            </StackPanel>
        </Grid>
        <Grid x:Name="Content" Grid.Column="0" Grid.Row="1" Margin="20">
            <DockPanel>
                <TabControl HorizontalAlignment="Left" MaxHeight="311" Width="Auto" VerticalAlignment="Top">
                    <TabItem Header="Boot">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.4*"/>
                                <ColumnDefinition Width="0.6*"/>
                            </Grid.ColumnDefinitions>
                            <Rectangle HorizontalAlignment="Center" VerticalAlignment="Center" Width="228" Height="228" Fill="#FFD6D6D6">
                                <Rectangle.OpacityMask>
                                    <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_cd}"></VisualBrush>
                                </Rectangle.OpacityMask>
                            </Rectangle>
                            <GroupBox Header="Boot" Grid.Column="1" Margin="10,10,10,0" >
                                <Grid Margin="10" VerticalAlignment="Center" HorizontalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="0.5*"/>
                                        <ColumnDefinition Width="0.5*"/>
                                    </Grid.ColumnDefinitions>
                                    <Controls:Tile x:Name="BootChoose" Grid.Column="0"  Click="BootChoose_Click">
                                        <StackPanel>
                                            <Rectangle Width="64" Height="64">
                                                <Rectangle.Fill>
                                                    <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_folder_open}"></VisualBrush>
                                                </Rectangle.Fill>
                                            </Rectangle>
                                        </StackPanel>
                                    </Controls:Tile>
                                    <Controls:Tile x:Name="Boot" Grid.Column="1" Click="Boot_Click" >
                                        <StackPanel>
                                            <Rectangle Width="64" Height="64">
                                                <Rectangle.Fill>
                                                    <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_cd}"></VisualBrush>
                                                </Rectangle.Fill>
                                            </Rectangle>
                                        </StackPanel>
                                    </Controls:Tile>

                                </Grid>

                            </GroupBox>
                        </Grid>
                    </TabItem>
                    <TabItem Header="Bootloader">
                        <Grid>
                            <DockPanel>
                                <Rectangle Width="180" Height="200" Fill="#FFD6D6D6">
                                    <Rectangle.OpacityMask>
                                        <VisualBrush Visual="{StaticResource appbar_box}"></VisualBrush>
                                    </Rectangle.OpacityMask>
                                </Rectangle>
                                <GroupBox Header="Lock/Unlock" Width="214">
                                    <StackPanel>
                                        <StackPanel>
                                            <Button x:Name="Lock"  Foreground="Gray" Click="Lock_Click" Margin="0,0,0,5">
                                                <Rectangle Width="30" Height="30" Fill="#FF706D6D">
                                                    <Rectangle.OpacityMask>
                                                        <VisualBrush Visual="{StaticResource appbar_lock}" Stretch="Fill"/>
                                                    </Rectangle.OpacityMask>
                                                </Rectangle>
                                            </Button>
                                            <Button x:Name="Unlock" Foreground="Gray" Click="Unlock_Click">
                                                <Rectangle Width="30" Height="30" Fill="#FF706D6D">
                                                    <Rectangle.OpacityMask>
                                                        <VisualBrush Visual="{StaticResource appbar_unlock}" Stretch="Fill"/>
                                                    </Rectangle.OpacityMask>
                                                </Rectangle>
                                            </Button>
                                        </StackPanel>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Header="Identifier TOKEN" Width="222">
                                    <DockPanel>
                                        <Button x:Name="GetIdentifierToken" VerticalAlignment="Top" Style="{DynamicResource MetroCircleButtonStyle}" Width="200" Height="200" Click="GetIdentifierToken_Click" Margin="5">
                                            <StackPanel Orientation="Vertical">
                                                <Rectangle Width="120" Height="120">
                                                    <Rectangle.Fill>
                                                        <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_barcode}" />
                                                    </Rectangle.Fill>
                                                </Rectangle>
                                                <TextBlock Text="GET TOKEN" />
                                            </StackPanel>
                                        </Button>

                                    </DockPanel>
                                </GroupBox>
                            </DockPanel>
                        </Grid>
                    </TabItem>
                    <TabItem Header="Erase">
                        <Grid>
                            <DockPanel>
                                <Rectangle Width="228" Height="228"  Fill="#FFD6D6D6">
                                    <Rectangle.OpacityMask>
                                        <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_cancel}"></VisualBrush>
                                    </Rectangle.OpacityMask>
                                </Rectangle>
                                <GroupBox Header="Erase">
                                    <StackPanel>
                                        <Button  Margin="0,0,0,5" x:Name="EraseSystem"  Click="EraseSystem_Click">
                                            <DockPanel>
                                                <Rectangle Width="20" Height="20" Fill="Gray">
                                                    <Rectangle.OpacityMask>
                                                        <VisualBrush Visual="{StaticResource appbar_delete}" Stretch="Fill"/>
                                                    </Rectangle.OpacityMask>
                                                </Rectangle>
                                                <TextBlock Foreground="Gray" Text="Erase System"/>
                                            </DockPanel>
                                        </Button>
                                        <Button  Margin="0,0,0,5" x:Name="EraseBoot" Click="EraseBoot_Click">
                                            <DockPanel>
                                                <Rectangle Width="20" Height="20" Fill="Gray">
                                                    <Rectangle.OpacityMask>
                                                        <VisualBrush Visual="{StaticResource appbar_delete}" Stretch="Fill"/>
                                                    </Rectangle.OpacityMask>
                                                </Rectangle>
                                                <TextBlock Foreground="Gray" Text="Erase Boot"/>
                                            </DockPanel>
                                        </Button>
                                        <Button  Margin="0,0,0,5" x:Name="EraseRecovery" Click="EraseRecovery_Click">
                                            <DockPanel>
                                                <Rectangle Width="20" Height="20" Fill="Gray">
                                                    <Rectangle.OpacityMask>
                                                        <VisualBrush Visual="{StaticResource appbar_delete}" Stretch="Fill"/>
                                                    </Rectangle.OpacityMask>
                                                </Rectangle>
                                                <TextBlock Foreground="Gray" Text="Erase Recovery"/>
                                            </DockPanel>
                                        </Button>
                                        <Button  Margin="0,0,0,5" x:Name="EraseCache"  Click="EraseCache_Click">
                                            <DockPanel>
                                                <Rectangle Width="20" Height="20" Fill="Gray">
                                                    <Rectangle.OpacityMask>
                                                        <VisualBrush Visual="{StaticResource appbar_delete}" Stretch="Fill"/>
                                                    </Rectangle.OpacityMask>
                                                </Rectangle>
                                                <TextBlock Foreground="Gray" Text="Erase Cache"/>
                                            </DockPanel>
                                        </Button>
                                        <Button  Margin="0,0,0,5" x:Name="EraseUserdata" Click="EraseUserdata_Click">
                                            <DockPanel>
                                                <Rectangle Width="20" Height="20" Fill="Gray">
                                                    <Rectangle.OpacityMask>
                                                        <VisualBrush Visual="{StaticResource appbar_delete}" Stretch="Fill"/>
                                                    </Rectangle.OpacityMask>
                                                </Rectangle>
                                                <TextBlock Foreground="Gray" Text="Erase UserData"/>
                                            </DockPanel>
                                        </Button>
                                    </StackPanel>
                                </GroupBox>
                            </DockPanel>

                        </Grid>
                    </TabItem>
                    <TabItem Header="Flash">
                        <Grid>
                            <DockPanel>
                                <Rectangle Width="228" Height="228" Fill="{DynamicResource AccentColorBrush}">
                                    <Rectangle.OpacityMask>
                                        <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_lightning}"></VisualBrush>
                                    </Rectangle.OpacityMask>
                                </Rectangle>
                                <GroupBox Header="Flash">
                                    <StackPanel>
                                        <DockPanel  Margin="0,0,0,5">
                                            <Button x:Name="FlashSystemChoose" Style="{StaticResource MetroAccentButton}" Click="FlashSystemChoose_Click">
                                                <Rectangle Width="24" Height="22">
                                                    <Rectangle.Fill>
                                                        <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_folder_open}" />
                                                    </Rectangle.Fill>
                                                </Rectangle>
                                            </Button>
                                            <Button x:Name="FlashSystem" Content="Flash System"  Foreground="Gray" Click="FlashSystem_Click"></Button>
                                        </DockPanel>
                                        <DockPanel  Margin="0,0,0,5">
                                            <Button x:Name="FlashBootChoose" Style="{StaticResource MetroAccentButton}" Click="FlashBootChoose_Click">
                                                <Rectangle Width="24" Height="22">
                                                    <Rectangle.Fill>
                                                        <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_folder_open}" />
                                                    </Rectangle.Fill>
                                                </Rectangle>
                                            </Button>
                                            <Button x:Name="FlashBoot" Content="Flash Boot"  Foreground="Gray" Click="FlashBoot_Click"></Button>
                                        </DockPanel>
                                        <DockPanel  Margin="0,0,0,5">
                                            <Button x:Name="FlashRecoveryChoose" Style="{StaticResource MetroAccentButton}" Click="FlashRecoveryChoose_Click">
                                                <Rectangle Width="24" Height="22">
                                                    <Rectangle.Fill>
                                                        <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_folder_open}" />
                                                    </Rectangle.Fill>
                                                </Rectangle>
                                            </Button>
                                            <Button x:Name="FlashRecovery" Content="Flash Recovery"  Foreground="Gray" Click="FlashRecovery_Click"></Button>
                                        </DockPanel>
                                        <DockPanel  Margin="0,0,0,5">
                                            <Button x:Name="FlashBootloaderChoose" Style="{StaticResource MetroAccentButton}" Click="FlashBootloaderChoose_Click">
                                                <Rectangle Width="24" Height="22">
                                                    <Rectangle.Fill>
                                                        <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_folder_open}" />
                                                    </Rectangle.Fill>
                                                </Rectangle>
                                            </Button>
                                            <Button x:Name="FlashBootloader" Content="Flash Bootloader"  Foreground="Gray" Click="FlashBootloader_Click"></Button>
                                        </DockPanel>
                                        <DockPanel  Margin="0,0,0,5">
                                            <Button x:Name="FlashRadioChoose" Style="{StaticResource MetroAccentButton}" Click="FlashRadioChoose_Click">
                                                <Rectangle Width="24" Height="22">
                                                    <Rectangle.Fill>
                                                        <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_folder_open}" />
                                                    </Rectangle.Fill>
                                                </Rectangle>
                                            </Button>
                                            <Button x:Name="FlashRadio" Content="Flash Radio"  Foreground="Gray" Click="FlashRadio_Click"></Button>
                                        </DockPanel>

                                    </StackPanel>
                                </GroupBox>
                                <Grid>
                                    <GroupBox Header="Flash" Margin="5,5,10,5">
                                        <Grid>
                                            <StackPanel>
                                                <DockPanel  Margin="0,0,0,5">
                                                    <Button x:Name="FlashUnlockTokenChoose" Style="{StaticResource MetroAccentButton}" Click="FlashUnlockTokenChoose_Click">
                                                        <Rectangle Width="24" Height="22">
                                                            <Rectangle.Fill>
                                                                <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_folder_open}" />
                                                            </Rectangle.Fill>
                                                        </Rectangle>
                                                    </Button>
                                                    <Button x:Name="FlashUnlockToken" Content="Flash Unlock Token"  Foreground="Gray" Click="FlashUnlockToken_Click" ></Button>
                                                </DockPanel>
                                                <DockPanel  Margin="0,0,0,5">
                                                    <Button x:Name="FlashUserdataChoose" Style="{StaticResource MetroAccentButton}" Click="FlashUserdataChoose_Click">
                                                        <Rectangle Width="24" Height="22">
                                                            <Rectangle.Fill>
                                                                <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_folder_open}" />
                                                            </Rectangle.Fill>
                                                        </Rectangle>
                                                    </Button>
                                                    <Button x:Name="FlashUserdata" Content="Flash Userdata"  Foreground="Gray" Click="FlashUserdata_Click"></Button>
                                                </DockPanel>
                                            </StackPanel>


                                        </Grid>

                                    </GroupBox>
                                </Grid>

                            </DockPanel>
                        </Grid>
                    </TabItem>
                    <TabItem Header="CID">
                        <Grid>
                            <DockPanel>
                                <Rectangle Width="228" Height="228" Fill="#FFD6D6D6">
                                    <Rectangle.OpacityMask>
                                        <VisualBrush Visual="{StaticResource appbar_vector_pen}" Stretch="Fill"></VisualBrush>
                                    </Rectangle.OpacityMask>
                                </Rectangle>
                                <GroupBox Header="CID OPERATIONS" Width="362">
                                    <StackPanel>
                                        <TextBox  Margin="0,0,0,5" x:Name="WriteCIDBlock" Controls:TextboxHelper.ClearTextButton="True" Controls:TextboxHelper.Watermark="ENTER CID"></TextBox>
                                        <Button  Margin="0,0,0,5" x:Name="WriteCid" Style="{StaticResource MetroAccentButton}" Click="WriteCid_Click">
                                            <DockPanel>
                                                <Rectangle Width="20" Height="20" Fill="White">
                                                    <Rectangle.OpacityMask>
                                                        <VisualBrush Visual="{StaticResource appbar_vector_pen}" Stretch="Fill"/>
                                                    </Rectangle.OpacityMask>
                                                </Rectangle>
                                                <TextBlock Foreground="White" Text="Write CID"/>
                                            </DockPanel>
                                        </Button>
                                        <Button x:Name="ReadCID" VerticalAlignment="Top" Style="{DynamicResource MetroCircleButtonStyle}" Width="95" Height="95" Click="ReadCID_Click">
                                            <StackPanel Orientation="Vertical">
                                                <Rectangle Width="45" Height="45">
                                                    <Rectangle.Fill>
                                                        <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_barcode}" />
                                                    </Rectangle.Fill>
                                                </Rectangle>
                                                <TextBlock Text="Read CID" />
                                            </StackPanel>
                                        </Button>
                                    </StackPanel>
                                </GroupBox>

                            </DockPanel>
                        </Grid>
                    </TabItem>
                    <TabItem Header="Execute">
                        <Grid>
                            <DockPanel>
                                <Rectangle Width="140" Height="140" Fill="#FF41B1E1">
                                    <Rectangle.OpacityMask>
                                        <VisualBrush Visual="{StaticResource appbar_alien}" Stretch="Fill"></VisualBrush>
                                    </Rectangle.OpacityMask>
                                </Rectangle>
                                <GroupBox Header="COMMANDS" Width="310">
                                    <Grid>

                                        <DockPanel>
                                            <StackPanel>
                                                <TextBox x:Name="Command1" Controls:TextboxHelper.Watermark="COMMAND1" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                                <TextBox x:Name="Command2" Controls:TextboxHelper.Watermark="COMMAND2" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                                <TextBox x:Name="Command3" Controls:TextboxHelper.Watermark="COMMAND3" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                                <TextBox x:Name="Command4" Controls:TextboxHelper.Watermark="COMMAND4" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                                <TextBox x:Name="Command5" Controls:TextboxHelper.Watermark="COMMAND5" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                            </StackPanel>
                                            <StackPanel>
                                                <TextBox x:Name="Command6" Controls:TextboxHelper.Watermark="COMMAND6" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                                <TextBox x:Name="Command7" Controls:TextboxHelper.Watermark="COMMAND7" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                                <TextBox x:Name="Command8" Controls:TextboxHelper.Watermark="COMMAND8" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                                <TextBox x:Name="Command9" Controls:TextboxHelper.Watermark="COMMAND9" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                                <TextBox x:Name="Command10" Controls:TextboxHelper.Watermark="COMMAND10" Foreground="Gray" Width="148"  Margin="0,0,0,5" Controls:TextboxHelper.ClearTextButton="True"/>
                                            </StackPanel>
                                        </DockPanel>

                                    </Grid>
                                </GroupBox>
                                <GroupBox Header="EXECUTE" Width="168" >
                                    <Grid>
                                        <StackPanel>
                                            <Controls:Tile x:Name="ExecuteCommands"  Click="ExecuteCommands_Click">
                                                <StackPanel>
                                                    <Rectangle Width="64" Height="64">
                                                        <Rectangle.Fill>
                                                            <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_check}"></VisualBrush>
                                                        </Rectangle.Fill>
                                                    </Rectangle>
                                                </StackPanel>
                                            </Controls:Tile>
                                        </StackPanel>
                                    </Grid>
                                </GroupBox>
                            </DockPanel>
                        </Grid>
                    </TabItem>
                    <TabItem Header="Other">
                        <DockPanel>
                            <Rectangle Width="180" Height="180" Fill="#FFD6D6D6">
                                <Rectangle.OpacityMask>
                                    <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_list1}"/>
                                </Rectangle.OpacityMask>
                            </Rectangle>
                            <GroupBox Header="Other" Width="451">
                                <DockPanel>
                                    <Controls:Tile x:Name="OpenZipFlasher" Click="OpenZipFlasher_Click">
                                        <StackPanel>
                                            <Rectangle Width="80" Height="80">
                                                <Rectangle.Fill>
                                                    <VisualBrush Stretch="Fill">
                                                        <VisualBrush.Visual>
                                                            <Canvas Width="48.0067" Height="48.0067" Clip="F1 M 0,0L 48.0067,0L 48.0067,48.0067L 0,48.0067L 0,0" UseLayoutRounding="False">
